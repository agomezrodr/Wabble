<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cs4330.cs.utep.edu.wabble">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:label="Wabble"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Sodas"></activity>
        <activity android:name=".Frappe"></activity>
        <activity android:name=".Salad"></activity>
        <activity android:name=".Sandwich"></activity>
        <activity android:name=".Waffle"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.DEFAULT"/>

            </intent-filter>
        </activity>
        <activity android:name=".Splash"
            android:label="Wabble"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>

            </intent-filter>

        </activity>
    </application>
</manifest>